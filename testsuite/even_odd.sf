let rec even (n : Int) : Int = if0 n - 2 then 1 else if0 n     then 1 else odd  (n - 1)
and     odd  (n : Int) : Int = if0 n - 3 then 1 else if0 n - 1 then 1 else even (n - 1)
in
odd 13

