-- System F code

let rec evenodd (select : Int) : Int -> Int =
    if select == 0 then
        \(n : Int). if n == 0 then 1 else if n == 1 then 0 else evenodd 1 (n-1)
    else
        \(n : Int). if n == 0 then 0 else if n == 1 then 1 else evenodd 0 (n-1)
in
evenodd 1 7
